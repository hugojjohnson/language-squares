{
  "name": "language-squares",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://hugojjohnson.github.io/language-squares",
  "type": "module",
  "scripts": {
    "check-env": "ts-node types/check-node-env.ts && ts-node types/check-vite-env.ts",
    "dev": "npm run check-env && vite",
    "build": "npm run check-env && tsc && vite build",
    "test-server": "NODE_ENV=production vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.7.2",
    "pinyin": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^7.1.9"
  }
}
